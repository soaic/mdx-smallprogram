<!--pages/setting/setting.wxml-->
<view class='root_view'>
  <view class='item_view' bindtap='onHomeClick'>
    <image src='/images/menu_home.png' style='width:32.5px;height:28px; margin-right:20px'></image>
    <view>主页</view>
  </view>
  <view class='item_view' bindtap='onAddClick'>
    <image src='/images/menu_create.png' style='width:32.5px;height:35px; margin-right:20px'></image>
    <view>增加预设</view>
  </view>
  <view class='item_view' bindtap='onSettingClick'>
  <image src='/images/menu_settings.png' style='width:32.5px;height:32.5px; margin-right:20px'></image>
    <view>设置</view>
  </view>
</view>
